___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Load Required Libraries
const query = require('queryPermission');
const log = require('logToConsole');
const injectScript = require('injectScript');
// Read Config Details
const apiKey = data.apiKey;
// Build the Script URL
const scriptUrl = 'https://cdn.provesrc.com/provesrc.js?apiKey='+apiKey;
// Inject the tracking Script into the Page
if (query('inject_script', scriptUrl)) {
  log('Injecting ProveSource script with key', apiKey);
  injectScript(scriptUrl, data.gtmOnSuccess,data.gtmOnFailure);
}else{
  log('No permission to inject script');
}


